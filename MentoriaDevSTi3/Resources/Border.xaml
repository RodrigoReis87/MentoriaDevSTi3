<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="BordaMenu" TargetType="Border">
        <Setter Property="BorderBrush" Value="#FF1B4561" />
        <Setter Property="BorderThickness" Value="0,0,0,1" />
    </Style>

    <Style x:Key="BrdTituloTelas" TargetType="Border">
        <Setter Property="Background" Value="#032338"/>
        <Setter Property="Margin" Value="5,37,5,0" />
        <Setter Property="BorderThickness" Value="0,0,0,1" />
    </Style>

    <Style x:Key="BrdSeparadorItens" TargetType="Border">
        <Setter Property="BorderBrush" Value="LightGray"/>
        <Setter Property="BorderThickness" Value="0,0,0,1"/>
    </Style>
</ResourceDictionary>